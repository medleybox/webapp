{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "7.3",
        "ocramius/proxy-manager": "~2.2.0",
        "sensio/framework-extra-bundle": "^5",
        "symfony/asset": "^4",
        "symfony/console": "^4",
        "symfony/expression-language": "^4",
        "symfony/flex": "^1.3.1",
        "symfony/form": "^4",
        "symfony/framework-bundle": "^4",
        "symfony/http-client": "^4",
        "symfony/intl": "^4",
        "symfony/monolog-bundle": "^3.4",
        "symfony/orm-pack": "^1",
        "symfony/proxy-manager-bridge": "4.*.*",
        "symfony/security-bundle": "^4",
        "symfony/swiftmailer-bundle": "^3",
        "symfony/translation": "^4",
        "symfony/twig-bundle": "^4",
        "symfony/validator": "^4",
        "symfony/web-link": "^4",
        "symfony/webpack-encore-bundle": "^1.6",
        "symfony/yaml": "^4",
        "xigen.io/guzzle-bundle": "^1.1"
    },
    "require-dev": {
        "symfony/debug-pack": "^1",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "^1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "php": "7.3"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "~4.4.7"
        }
    }
}
