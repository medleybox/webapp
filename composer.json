{
    "name": "medleybox/webapp",
    "description": "A Symfony 6 application to serve the frontend of Medleybox.",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=8.3",
        "composer-plugin-api": "^2",
        "doctrine/doctrine-bundle": "^2",
        "doctrine/orm": "^2",
        "meilisearch/search-bundle": "~0.14",
        "nyholm/psr7": "^1.7",
        "symfony/asset": "^6",
        "symfony/cache": "^6",
        "symfony/console": "^6",
        "symfony/filesystem": "^6",
        "symfony/flex": "~2",
        "symfony/form": "^6",
        "symfony/framework-bundle": "^6",
        "symfony/http-client": "^6",
        "symfony/intl": "^6",
        "symfony/mailer": "^6",
        "symfony/monolog-bundle": "^3.5",
        "symfony/proxy-manager-bridge": "^6",
        "symfony/runtime": "^6",
        "symfony/security-bundle": "^6",
        "symfony/twig-bundle": "^6",
        "symfony/validator": "^6",
        "symfony/web-link": "^6",
        "symfony/webpack-encore-bundle": "~2.1",
        "symfony/yaml": "^6"
    },
    "require-dev": {
        "composer/composer": "^2.3",
        "phpstan/extension-installer": "^1.2",
        "phpstan/phpstan": "^1.6",
        "phpstan/phpstan-doctrine": "^1.3",
        "phpstan/phpstan-symfony": "^1.2",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/debug-bundle": "^6",
        "symfony/maker-bundle": "1.43.0",
        "symfony/phpunit-bridge": "^6",
        "symfony/stopwatch": "^6",
        "symfony/web-profiler-bundle": "^6"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "php": "8.3"
        },
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/runtime": true,
            "symfony/flex": true,
            "phpstan/extension-installer": true,
            "php-http/discovery": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "doctrine/migrations": "*",
        "doctrine/doctrine-migrations-bundle": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php83": "*",
        "symfony/polyfill-php82": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "^6.4"
        }
    }
}
