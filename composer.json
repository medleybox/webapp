{
    "name": "mewdleybox/webapp",
    "description": "A Symfony 5 application to serve the frontend of Medleybox.",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=8.0",
        "composer-plugin-api": "^2",
        "doctrine/doctrine-bundle": "^2",
        "doctrine/orm": "^2",
        "k911/swoole-bundle": "^0.10.0",
        "sensio/framework-extra-bundle": "^6.2",
        "symfony/asset": "^5",
        "symfony/console": "^5",
        "symfony/flex": "^1.12.2",
        "symfony/form": "^5",
        "symfony/framework-bundle": "^5",
        "symfony/http-client": "^5",
        "symfony/intl": "^5",
        "symfony/mailer": "^5",
        "symfony/monolog-bundle": "^3.5",
        "symfony/proxy-manager-bridge": "^5",
        "symfony/runtime": "^5",
        "symfony/security-bundle": "^5",
        "symfony/sendgrid-mailer": "^5",
        "symfony/twig-bundle": "^5",
        "symfony/validator": "^5",
        "symfony/web-link": "^5",
        "symfony/webpack-encore-bundle": "^1.12",
        "symfony/yaml": "^5"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.62",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/debug-bundle": "^5",
        "symfony/maker-bundle": "^1.19",
        "symfony/phpunit-bridge": "^5",
        "symfony/stopwatch": "^5",
        "symfony/web-profiler-bundle": "^5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "php": "8.0"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "doctrine/migrations": "*",
        "doctrine/doctrine-migrations-bundle": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "~5.3"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
