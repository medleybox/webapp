#!/bin/bash

function dclocal(){
    docker-compose -f docker-compose.yml -f docker/docker-compose-dev.yml $@
}

function dcConsole(){
    docker-compose -f docker-compose.yml exec webapp bin/console -vvv $@
}

dclocal up -d --force-recreate

dcConsole cache:clear
dcConsole cache:warmup
dcConsole doctrine:schema:update --force

dclocal logs -f webapp
